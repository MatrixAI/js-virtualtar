{
  "name": "@matrixai/js-virtualtar",
  "version": "2.0.0",
  "author": "Matrix AI",
  "contributors": [
    {
      "name": "Aryan Jassal"
    }
  ],
  "description": "Virtualised bindings for generating a tar file",
  "keywords": [
    "tar",
    "fs",
    "file",
    "tarball",
    "directory",
    "stream"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-virtualtar.git"
  },
  "type" :"module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./*": "./dist/*"
  },
  "imports": {
    "#*": "./dist/*"
  },
  "scripts": {
    "prepare": "tsc -p ./tsconfig.build.json",
    "build": "shx rm -rf ./dist && tsc -p ./tsconfig.build.json",
    "postversion": "npm install --package-lock-only --ignore-scripts --silent",
    "tsx": "tsx",
    "test": "node ./scripts/test.mjs",
    "lint": "matrixai-lint --shell src tests",
    "lintfix": "matrixai-lint --fix --shell src tests",
    "docs": "shx rm -rf ./docs && typedoc --gitRevision master --tsconfig ./tsconfig.build.json --out ./docs src"
  },
  "dependencies": {
    "@matrixai/errors": "^2.1.3"
  },
  "devDependencies": {
    "@fast-check/jest": "^2.1.0",
    "@matrixai/lint": "^0.2.11",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.1.3",
    "@types/node": "^18.15.0",
    "@types/tar": "^6.1.13",
    "fast-check": "^3.0.1",
    "jest": "^28.1.1",
    "jest-extended": "^3.0.1",
    "jest-junit": "^14.0.0",
    "pkg": "^5.8.1",
    "shx": "^0.3.4",
    "tar": "^7.4.3",
    "tsx": "^3.12.7",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  }
}
