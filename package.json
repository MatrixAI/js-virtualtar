{
  "name": "@matrixai/js-virtualtar",
  "version": "2.0.0",
  "author": "Matrix AI",
  "contributors": [
    {
      "name": "Aryan Jassal"
    }
  ],
  "description": "Virtualised bindings for generating a tar file",
  "keywords": [
    "tar",
    "fs",
    "file",
    "tarball",
    "directory",
    "stream"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-virtualtar.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc -p ./tsconfig.build.json",
    "build": "shx rm -rf ./dist && tsc -p ./tsconfig.build.json",
    "postversion": "npm install --package-lock-only --ignore-scripts --silent",
    "ts-node": "ts-node",
    "test": "jest",
    "lint": "matrixai-lint --shell src tests",
    "lintfix": "matrixai-lint --fix --shell src tests",
    "docs": "shx rm -rf ./docs && typedoc --gitRevision master --tsconfig ./tsconfig.build.json --out ./docs src"
  },
  "dependencies": {
    "@matrixai/errors": "^1.1.7"
  },
  "devDependencies": {
    "@fast-check/jest": "^1.1.0",
    "@matrixai/lint": "^0.2.8",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.15.0",
    "@types/tar": "^6.1.13",
    "eslint": "^9.0.0",
    "fast-check": "^3.0.1",
    "jest": "^28.1.1",
    "jest-extended": "^3.0.1",
    "jest-junit": "^14.0.0",
    "pkg": "^5.8.1",
    "shx": "^0.3.4",
    "tar": "^7.4.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  }
}
